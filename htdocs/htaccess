# This file didn't have a dot because we didn't want to overwrite
# your actual .htaccess file.  You should rename this file to .htaccess if you
# didn't already do so.

RewriteEngine On

# RewriteBase should be the relative path by which your install is reachable
# on the server, e.g. if your Habari blog were at http://example.com/habari/,
# RewriteBase would be /habari/.
RewriteBase /

RewriteCond %{REQUEST_FILENAME} !-f
RewriteRule . index.php [PT]
